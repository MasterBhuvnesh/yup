{
  "name": "expo-push-token-server",
  "version": "1.0.0",
  "description": "A Node.js server to store and retrieve Expo push tokens using Firebase.",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "nodemon",
    "build": "node scripts/build.js",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "keywords": [
    "expo",
    "push-notifications",
    "firebase",
    "nodejs",
    "typescript"
  ],
  "author": "Bhuvnesh Verma",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "axios": "^1.11.0",
    "dotenv": "^17.2.1",
    "express": "^4.17.1",
    "firebase-admin": "^10.3.0",
    "joi": "^17.13.3",
    "node-cron": "^4.2.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@types/axios": "^0.9.36",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.13",
    "@types/jest": "^30.0.0",
    "@types/node": "^17.0.21",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.38.0",
    "@typescript-eslint/parser": "^8.38.0",
    "esbuild": "^0.25.8",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "jest": "^30.0.5",
    "nodemon": "^2.0.15",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.7.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^4.6.2"
  }
}
